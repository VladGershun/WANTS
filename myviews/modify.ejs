	<script type="text/javascript">

		function forceInputUppercase(e)
		{
			var start = e.target.selectionStart;
			var end = e.target.selectionEnd;
			e.target.value = e.target.value.toUpperCase();
			e.target.setSelectionRange(start, end);
		}
		document.getElementById("sfirstinput").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("slastinput").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("firstinput").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("lastinput").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("middleinput").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("chargeinput").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("chargeinput2").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("chargeinput3").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("warrantinput").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("warrantinput2").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("warrantinput3").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("tripleginput").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("tripleginput2").addEventListener("keyup", forceInputUppercase, false);
		document.getElementById("tripleginput3").addEventListener("keyup", forceInputUppercase, false);
	
	</script>

