		}

	</script>

<script>
	var count=0;
	let total=0

	let date1 = document.getElementById('tripdateinput').addEventListener("change", function(event) {
			date1=event.target.value;
	});
	let date2 = document.getElementById('tripdateinput2').addEventListener("change", function(event) {
			date2=event.target.value;
	});
	let date3 = document.getElementById('tripdateinput3').addEventListener("change", function(event) {
			date3=event.target.value;
	});


	let flag = 0;
	
	function display(date1, date2, date3) {
		let table = document.getElementById('addinfo');
		let leg1 = document.getElementById('tripleginput').value;	
		let leg2 = document.getElementById('tripleginput2').value;	
		let leg3 = document.getElementById('tripleginput3').value;		
		let c, r, t, c2, r2, t2, c3, r3, t3;
	
		t = document.createElement('table');
		r = t.insertRow(0); 
		c = r.insertCell(0);
		c.innerHTML = date1;
		c = r.insertCell(1);
		c.innerHTML = leg1.toUpperCase();

		t2 = document.createElement('table');
		r2 = t2.insertRow(0); 
		c2 = r2.insertCell(0);
		c2.innerHTML = date2;
		c2 = r2.insertCell(1);
		c2.innerHTML = leg2.toUpperCase();

		t3 = document.createElement('table');
		r3 = t3.insertRow(0); 
		c3 = r3.insertCell(0);
		c3.innerHTML = date3;
		c3 = r3.insertCell(1);
		c3.innerHTML = leg3.toUpperCase();


		if(flag == 0) {
		if(date1 != null) document.getElementById("addinfo").appendChild(t);
		if(date2 != null) document.getElementById("addinfo").appendChild(t2);
		if(date3 != null) document.getElementById("addinfo").appendChild(t3);
		flag=1;
		}
	}

