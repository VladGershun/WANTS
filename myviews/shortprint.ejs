<!DOCTYPE html>
<html>

	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>Query | W.A.N.T.S</title>
		<link rel="stylesheet" type="text/css" href="css/print.css">
	</head>
	
	<body>
		<div class="info">
			<h2><%= title %></h2>
			<table>
				<thead>
					<tr>
						<th>LAST</th>
						<th>FIRST</th>
						<th>M</th>
						<th>DOB</th>
						<th>RACE</th>
						<th>SEX</th>
						<th>FROM</th>
						<th>DEST</th>
						<th>NOTES</th>
					</tr>
				</thead>
				<tbody>
						<% inmates.forEach(function(inmates){ %>
								<tr>
										<td><%= inmates.inmatelast %></td>
										<td><%= inmates.inmatefirst %></td>
										<td><%= inmates.inmatemiddle %></td>
										<td><%= moment(inmates.dob).format('MM/DD/YYYY') %></td>
										<td><%= inmates.race %></td>
										<td><%= inmates.sex %></td>
										<td><%= inmates.eagency %></td>
										<td><%= inmates.dagency %></td>
										<td><%= inmates.notes %></td>
								</tr>
						<% }); %>
				</tbody>
			</table>
		</div>


		<button class="button" onclick="window.location.href='/query'" type="button">Back</button>



	</body>

</html>










